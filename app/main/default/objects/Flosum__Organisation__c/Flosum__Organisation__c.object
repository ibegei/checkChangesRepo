        <linkType>javascript</linkType>
        <masterLabel>Enable/Disable Bitbucket Synchronize</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/22.0/connection.js&quot;)} 
var newRecords = []; 
var c = new sforce.SObject(&quot;Flosum__Organisation__c&quot;); 
c.id =&quot;{!Flosum__Organisation__c.Id}&quot;; 
console.log(&apos;@@@@@@&apos;);
var q = {!Flosum__Organisation__c.Bitbucket_Synchronize__c};
console.log(q);
if({!Flosum__Organisation__c.Bitbucket_Synchronize__c}==0){
c.Bitbucket_Synchronize__c= true;
}else{
c.Bitbucket_Synchronize__c= false;
}

newRecords.push(c); 
result = sforce.connection.update(newRecords); 
window.location.reload();</url>
